#!/usr/bin/env bash

screen="${1}" && cmd="${2}" && shift 2
# ----------------------

mode=${1}
echo "$screen $mode" 
xrandr  --output eDP1 --primary         \
        --output ${screen} --above eDP1 \
        --mode          ${mode}         \
        --dpi           96              \
        --rate          60.00           \
        --rotate        normal
